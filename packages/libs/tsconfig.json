    "outDir": "dist",
    "declaration": true,
    "jsx": "react-jsx",
    "module": "ESNext",
    "resolveJsonModule": true,
    "target": "ESNext",
    "moduleResolution": "Node",
    "baseUrl": "./",
    "esModuleInterop": true,
    // "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
