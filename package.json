{
  "name": "me",
  "version": "1.0.0",
  "description": "Fastify + Drizzle app",
  "main": "dist/main.js",
  "type": "commonjs",
  "scripts": {
    "dev": "tsx watch src/main.ts",
    "start": "node dist/main.js",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint \"src/**/*.{ts,js}\"",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "typeorm": "typeorm-ts-node-commonjs",
    "mig:gen": "npm run typeorm -- migration:generate src/migrations/Create -d src/typeorm.config.ts",
    "mig:run": "npm run typeorm -- migration:run -d src/typeorm.config.ts",
    "mig:revert": "npm run typeorm -- migration:revert -d src/typeorm.config.ts"
  },
  "dependencies": {
    "@fastify/helmet": "^13.0.1",
    "@fastify/jwt": "^9.1.0",
    "@fastify/rate-limit": "^10.2.2",
    "@fastify/sensible": "^6.0.3",
    "@fastify/swagger": "^9.5.0",
    "@fastify/swagger-ui": "^5.2.2",
    "@types/http-errors": "^2.0.4",
    "dotenv": "^16.5.0",
    "fastify": "^5.3.2",
    "fastify-guard": "^3.0.1",
    "fastify-type-provider-zod": "^4.0.2",
    "http-errors": "^2.0.0",
    "mysql2": "^3.14.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.22",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  }
}
