{
  "name": "prisma-app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "ts-node src/index.ts",
    "dev": "nest start --watch",
    "build": "nest build && node scripts/copy-keys.js",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docs": "redocly build-docs openapi.yaml --output=docs.html",
    "typeorm": "typeorm-ts-node-commonjs",
    "mig:gen": "npm run typeorm -- migration:generate src/migrations/CreateMigration -d typeorm.config.ts",
    "mig:run": "npm run typeorm -- migration:run -d typeorm.config.ts",
    "mig:revert": "npm run typeorm -- migration:revert -d typeorm.config.ts"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.0.1",
    "@fastify/multipart": "^9.0.3",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-fastify": "^11.1.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjsx/crud": "^5.0.0-alpha.3",
    "@nestjsx/crud-typeorm": "^5.0.0-alpha.3",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.5.0",
    "fastify": "^5.3.2",
    "fastify-mailer": "^2.3.1",
    "fastify-plugin": "^5.0.1",
    "mysql2": "^3.14.0",
    "nestjs-typeorm-paginate": "^4.1.0",
    "nodemailer": "^6.10.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pug": "^3.0.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@anatine/zod-nestjs": "^2.0.12",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@fastify/static": "^8.1.1",
    "@nestjs/cli": "^11.0.6",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/swagger": "^11.1.4",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.3",
    "@types/jest": "^29.5.14",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^22.14.1",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pug": "^2.0.10",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "openapi3-ts": "^4.4.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.22",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.20.0",
    "zod-to-json-schema": "^3.24.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
